<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="20dp"
    android:clickable="true"
    android:foreground="?attr/selectableItemBackground"
    >

    <LinearLayout
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-medium"
            android:text="id"
            android:textSize="10.5dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-medium"
            android:text="numero de control"
            android:textSize="10.5dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-medium"
            android:text="nombre"
            android:textSize="10.5dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-medium"
            android:text="carrera"
            android:textSize="10.5dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-medium"
            android:text="semestre"
            android:textSize="10.5dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-medium"
            android:text="grupo"
            android:textSize="10.5dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <View
            android:id="@+id/divider7"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_weight="1"
            android:background="?android:attr/listDivider" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/mostrar_id"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-medium"
            android:text="mostrar_id"
            android:textSize="10.5dp"
            android:typeface="normal" />

        <TextView
            android:id="@+id/mostrar_numero_control"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-medium"
            android:text="mostrar_numero_control"
            android:textSize="10.5dp"
            android:typeface="normal" />

        <TextView
            android:id="@+id/mostrar_nombre"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-medium"
            android:text="mostrar_nombre"
            android:textSize="10.5dp"
            android:typeface="normal" />

        <TextView
            android:id="@+id/mostrar_carrera"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-medium"
            android:text="mostrar_carrera"
            android:textSize="10.5dp"
            android:typeface="normal" />

        <TextView
            android:id="@+id/mostrar_semestre"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-medium"
            android:text="mostrar_semestre"
            android:textSize="10.5dp"
            android:typeface="normal" />

        <TextView
            android:id="@+id/mostrar_grupo"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif-medium"
            android:text="mostrar_grupo"
            android:textSize="10.5dp"
            android:typeface="normal" />

        <View
            android:id="@+id/divider6"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_weight="1"
            android:background="?android:attr/listDivider" />
    </LinearLayout>

</LinearLayout>